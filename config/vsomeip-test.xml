<!-- vsomeip test configuration file -->
<someip>
	<address>10.0.2.15</address>
	<logging> 
		<level>debug</level>
		<console>true</console>
		<file>
			<enabled>true</enabled>
			<path>/home/someip/another-file.log</path>
		</file>
		<dlt>false</dlt>
	</logging>
	<services>
		<servicegroup> 
			<name>default</name>
			<address>local</address>
