	</services>
	<routing>
		<host>A</host>
	</routing>
	<service-discovery>
		<host>my_application</host>
		<port>30666</port>
		<protocol>tcp</protocol>
	</service-discovery>
	<applications>
		<application>
			<name>A</name>
